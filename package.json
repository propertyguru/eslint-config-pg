{
  "name": "@propertyguru/eslint-config-pg",
  "version": "2.0.0",
  "description": "eslint rules for typescript code base",
  "main": "index.js",
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint ./ --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier ./ --check",
    "lint": "npm run eslint && npm run prettier",
    "fix": "npm run eslint -- --fix && npm run prettier -- --write",
    "print:eslint:config": "eslint --print-config src/javascript/common.js > './.eslintrc.js_fullsettings.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/propertyguru/eslint-config-pg.git"
  },
  "keywords": [
    "eslint",
    "prettier",
    "typescript"
  ],
  "author": "Agent Platform Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/propertyguru/eslint-config-pg/issues"
  },
  "homepage": "https://github.com/propertyguru/eslint-config-pg#readme",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-functional": "^5.0.8",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-functional": "^5.0.8",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  }
}
